doctype html
html
  head
    title #{title} - Techie.JS
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(src="http://code.jquery.com/jquery-1.11.0.min.js")
body
    script(type='text/javascript' src='helper.js')
    header
      h1 Techie.JS
      <a href = "localhost:3000"><img src="http://openclipart.org/image/800px/svg_to_png/182094/refresh-icon.png" href="localhost:3000" width="5%" height="5%"></a>

    #container
      <h2>Filter:<h2>
      <input type="text" id="filterText">
      <button onclick="doFilter()">Submit</button>
      p(style='white-space:pre;')
        |<br />

      #main-content

        block content
     #sidebar
        block sidebar
    footer
      p Fork us on GitHub: https://github.com/frankcash/Techie_JS
